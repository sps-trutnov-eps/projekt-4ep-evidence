@model AuthUser?

<label for="studyField">Obor:</label>
<select id="obory" name="studyField" required>
    @if(Model != null)
    {
        <option value="@Model.studyField" selected hidden>@Model?.studyField</option>
    }
    <option id="EPS" value="EPS">EPS</option>
    <option id="IT" value="IT">IT</option>
    <option id="ST" value="ST">ST</option>
    <option id="S" value="S">S</option>
    <option id="jine" value="jine">Jiné</option>
</select>
<label for="schoolYear">Ročník:</label>
<select id="rocniky" name="schoolYear" required>
    @if(Model != null)
    {
        <option value="@Model.schoolYear" selected hidden>@Model?.schoolYear</option>
    }
    <option id="1" value="1">1</option>
    <option id="2" value="2">2</option>
    <option id="3" value="3">3</option>
    <option id="4" value="4">4</option>
    <option id="5" value="5">Absolvent</option>
</select>